/**
 * And gate: out = 1 if {a == 1 and b == 1}, 0 otherwise  
 */

CHIP And3Way {

    IN  a, b, c;
    OUT out;

    PARTS:
    And(a=a,b=b,out=out1);
    And(a=out1,b=c,out=out);
}
